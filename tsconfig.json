{
	"compilerOptions": {
		"module": "esnext",
		"lib": ["es2019"],
		"target": "es2019",
		"moduleResolution": "Node",
		"outDir": "C:/Users/danvi/AppData/Local/Screeps/scripts/screeps_newbieland_net___21025/default",
		"baseUrl": "./src/",
		"sourceMap": false,
		"strict": true,
		"experimentalDecorators": true,
		"noImplicitReturns": true,
		"noImplicitAny": false,
		"allowSyntheticDefaultImports": true,
		"allowUnreachableCode": false,
		"paths": {
			"@globals": 	["./functions/utils/globals"],
			"@constants": 	["./functions/utils/constants"],
			"@protos/*":	["./prototypes/*"],
			"@prototypes/*":["./prototypes/*"],
			"@creeps/*": 	["./creeps/*"],
			"@classes/*": 	["./classes/*"],
			"@managers/*": 	["./managers/*"],
			"@modules/*": 	["./modules/*"],
			"@utils/*": 	["./functions/utils/*"],
			"@funcs/*": 	["./functions/*"],
			"@functions/*": ["./functions/*"],
			"@main": 		["./main"]
		}
	},
	"include": [ "src", "src/types/**/*.d.ts"],
	"exclude": [ "node_modules", "unused/**/*.ts" ],
}
